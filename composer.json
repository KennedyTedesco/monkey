{
  "name": "kennedytedesco/monkey",
  "description": "Monkey Programming Language",
  "license": "proprietary",
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.0",
    "pestphp/pest": "^0.1",
    "phpunit/phpunit": "^9.0",
    "vimeo/psalm": "^3.9"
  },
  "autoload": {
    "psr-4": {
      "Monkey\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "pest",
    "psalm": "psalm --show-info=false",
    "cs": "php-cs-fixer fix --allow-risky yes"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
